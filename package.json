{
    "name": "arkhe-woo",
    "version": "1.0.0",
    "description": " modules",
    "main": "index.js",
    "scripts": {
        "postinstall": "composer install && composer set-wpcs",
        "build:css": "npx gulp compileScss",
        "build:js": "npx gulp minifyJs",
        "build": "npm run build:js && npm run build:css",
        "watchs:css": "nodemon --watch src -e scss -x \"npm run build:css\"",
        "watch:js": "nodemon --watch src/js -e js -x \"npm run build:js\"",
        "watch": "npx npm-run-all --parallel watchs:css watch:js",
        "po2json": "po2json languages/arkhe-woo-ja.po languages/arkhe-woo-ja-arkhe-woo-admin.json -f jed"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/polyfill": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@wordpress/eslint-plugin": "^7.2.1",
        "babel-loader": "^8.1.0",
        "classnames": "^2.2.6",
        "core-js": "^3.6.5",
        "eslint": "^7.8.1",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-group-css-media-queries": "^1.2.2",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-sass-glob": "^1.1.0",
        "gulp-uglify-es": "^2.0.0",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4",
        "npm-check-updates": "^8.1.1",
        "npm-run-all": "^4.1.5",
        "po2json": "^1.0.0-beta-2",
        "prettier": "^2.1.2",
        "regenerator-runtime": "^0.13.7",
        "stylelint": "^13.7.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-config-wordpress": "^17.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.18.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {},
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "modules": false,
                    "useBuiltIns": "usage",
                    "corejs": 3
                }
            ]
        ]
    },
    "browserslist": [
        "> 0.5%",
        "last 2 versions",
        "ie >= 11",
        "not dead"
    ]
